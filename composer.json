{
  "name": "log1x/acf-composer",
  "type": "package",
  "description": "Create fields, blocks, option pages, and widgets using ACF Builder and Sage 10",
  "license": "MIT",
  "authors": [
    {
      "name": "Brandon Nifong",
      "email": "brandon@tendency.me"
    }
  ],
  "autoload": {
    "psr-4": {
